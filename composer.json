{
  "name": "spryker/phar",
  "description": "Project",
  "license": "MIT",
  "require": {
    "symfony/console": "^3.0.0",
    "symfony/finder": "^3.0.0",
    "symfony/process": "^3.0.0",
    "symfony/yaml": "^3.0.0",
    "seld/phar-utils": "^1.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.2.10",
    "symfony/var-dumper": "^3.0.0",
    "site5/phantoman": "^1.1",
    "jakoch/phantomjs-installer": "^2.1.0",
    "phpstan/phpstan": "^0.8.5",
    "spryker/code-sniffer": "dev-master as 1.0.0",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker",
      "PharBuilder\\": "src/PharBuilder"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "use-include-path": true,
    "process-timeout": 600
  }
}
